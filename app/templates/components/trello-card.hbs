{{!-- Loop through each card --}}
{{#each list.cards as |card|}}

{{!-- Populates modal --}}
{{#if enabled}}
  {{card-modal
    enabled=enabled
    title=card.name
    body=card.description
    card=card
    list=list
  }}
{{/if}}

  <div class="card-sm" id="remove-default-card-{{card.id}}" {{action 'openModal'}}>
    <div class='card-name'>{{card.name}}</div>
  </div>
{{/each}}
