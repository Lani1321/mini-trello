{{!-- Loop through each card --}}
{{#each list.cards as |card|}}

{{!-- Populates modal --}}
{{#if enabled}}
  {{card-modal
    enabled=enabled
    title=card.name
    body=card.description
    card=card
    list=list
  }}
{{/if}}

  <div class="card-sm" {{action 'openModal'}}>
    {{card.name}}
  </div>
{{/each}}
